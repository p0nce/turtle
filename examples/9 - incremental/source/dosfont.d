module dosfont;

import dplug.math.box;

import dplug.graphics.draw;
import dplug.graphics.image;

nothrow @nogc:

enum DOSFontType
{
    small5x5,   // 5x5 characters
    small8x8,   // 8x8 characters
    large8x16,  // 8x16 characters
}


int getDOSTextCharWidth(DOSFontType font,  float scaleFactor, int marginX = 0)
{
    if (font == DOSFontType.small5x5)
        return cast(int)(0.5f + scaleFactor * 5) + marginX;
    else
        return cast(int)(0.5f + scaleFactor * 8) + marginX;
}

int getDOSTextCharHeight(DOSFontType font,  float scaleFactor)
{
    int inCharHeight;
    if (font == DOSFontType.small5x5)
    {
        inCharHeight = 5;
    }
    else if (font == DOSFontType.small8x8)
    {
        inCharHeight = 8;
    }
    else if (font == DOSFontType.large8x16)
    {
        inCharHeight = 16;
    }
    else
        assert(false);

    return cast(int)(0.5f + scaleFactor * inCharHeight);
}

/// Draw a string using fonts dumped from DOS in olden times.
/// This is CP437. The input will be interpreted as CP437, not UTF-8!
/// Note: this use nearest sampling to scale the font bitmap, in order to feel oldschool.
void drawDOSText(ref ImageRef!RGBA surface, 
                 DOSFontType font, 
                 const(char)[] s, 
                 RGBA textColor, 
                 int positionX,
                 int positionY,
                 int marginX = 0,
                 float scaleFactor = 1.0f)
{
    // char size in the font
    int inCharWidth;
    int inCharHeight;

    // when scaleFactor is applied
    int outCharWidth;
    int outCharHeight;

    int bitMax;

    const(ubyte)* selectedFont;
    box2i surfaceRect = box2i(0, 0, surface.w, surface.h);


    if (font == DOSFontType.small5x5)
    {
        selectedFont = very_small_font_data.ptr;
        inCharHeight = 5;
        inCharWidth = 5;
        bitMax = 4;
    }
    else if (font == DOSFontType.small8x8)
    {
        selectedFont = small_font_data.ptr;
        inCharHeight = 8;
        inCharWidth = 8;
        bitMax = 7;
    }
    else if (font == DOSFontType.large8x16)
    {
        selectedFont = large_font_data.ptr;
        inCharHeight = 16;
        inCharWidth = 8;
        bitMax = 7;
    }
    else
        assert(false);

    outCharWidth = cast(int)(0.5f + scaleFactor * inCharWidth);
    outCharHeight = cast(int)(0.5f + scaleFactor * inCharHeight);

    float actualScaleX = scaleFactor;
    float actualScaleY = scaleFactor;

    // Whole string wise clipping
    box2i textRect = box2i.rectangle(positionX, positionY, outCharWidth*cast(int)(s.length), outCharHeight);
    if (!surfaceRect.intersects(textRect))
        return;

    void drawChar(char ch, int charX, int charY) nothrow @nogc
    {        
        int cropX0 = charX;
        int cropY0 = charY;
        int cropX1 = charX + outCharWidth;
        int cropY1 = charY + outCharHeight;
        if (cropX0 < 0) cropX0 = 0;
        if (cropX0 > surface.w) cropX0 = surface.w;
        if (cropY0 < 0) cropY0 = 0;
        if (cropY0 > surface.h) cropY0 = surface.h;
        if (cropX1 < 0) cropX1 = 0;
        if (cropX1 > surface.w) cropX1 = surface.w;
        if (cropY1 < 0) cropY1 = 0;
        if (cropY1 > surface.h) cropY1 = surface.h;

        // Early exit if out of scope
        if (!surfaceRect.intersects(box2i(cropX0, cropY0, cropX1, cropY1)))
            return;

        ubyte c = cast(ubyte)ch;
        const(ubyte) * charData = selectedFont + (inCharHeight * c);

        auto outSurf = surface.cropImageRef(cropX0, cropY0, cropX1, cropY1);
        int xOffset = cropX0 - charX;
        int yOffset = cropY0 - charY;

        for (int i = yOffset; i < cropY1 - charY; ++i)
        {
            RGBA[] scan = outSurf.scanline(i - yOffset);
            for (int j = xOffset; j < cropX1 - charX; ++j)
            {
                // where to sample the input?
                int iline = cast(int)(0.5f + i / actualScaleY);
                if (iline >= inCharHeight)
                    continue;
                int lineData = charData[iline];

                int icol = cast(int)(0.5f + j / actualScaleX);
                if (icol >= inCharWidth)
                    continue;
                if ((1 & (lineData >> (bitMax - icol))) != 0)
                {
                    blendFontPixel(scan[j - xOffset], textColor, textColor.a);
                }
            }
        }
    }  

    foreach(size_t i, char ch; s) // iterated by code unit, not codepoint
    {    
        drawChar(ch, positionX + (cast(int)i) * (outCharWidth + marginX), positionY);      
    }
}


